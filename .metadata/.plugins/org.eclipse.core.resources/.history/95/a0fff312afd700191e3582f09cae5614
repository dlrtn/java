abstract class Calc {
	int a = 5;
	int b = 6;
	
	abstract void plus();
}

class MyCalc extends Calc {
	void plus()  { System.out.println(a + b); }
	void minus() { System.out.println(a - b); }
}

public class Polymorphism1 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
